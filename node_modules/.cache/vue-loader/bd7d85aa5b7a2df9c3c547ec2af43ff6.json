ns } from 'vuex'\nexport default {\n  components: {\n    [AddressEdit.name]: AddressEdit,\n    [NavBar.name]: NavBar,\n    [Icon.name]: Icon,\n    Icon\n  },\n  data() {\n    return {\n      areaList: AreaList,\n      searchResult: [],\n      addressInfo: {}\n    }\n  },\n  computed: {\n    ...mapGetters(['getCurrentAddressInfo'])\n  },\n  created() {\n    console.log('getAddress')\n    console.log(this.getCurrentAddressInfo)\n    this.addressInfo = this.getCurrentAddressInfo\n  },\n  methods: {\n    onSave() {\n      Toast('save')\n    },\n    onDelete() {\n      Toast('delete')\n    },\n    onChangeDetail(val) {\n      console.log('val')\n      console.log(val)\n      if (val) {\n        this.searchResult = [\n          {\n            name: '黄龙万科中心',\n            address: '杭州市西湖区'\n          }\n        ]\n      } else {\n        this.searchResult = []\n      }\n    }\n  }\n}\n</script>\n\n<style lang=\"less\" scope>\n\n.my-swipe {\n  .van-swipe-item {\n    color: #fff;\n    font-size: 20px;\n    line-height: 150px;\n    text-align: center;\n    top:40px;\n    background-color:#fff;\n    height: 300px;\n    img {\n      display: block;\n      box-sizing: border-box;\n      width: 100%;\n      height: 240px;\n      padding: 30px 60px;\n      background-color: #fff;\n      pointer-events: none;\n    }\n  }\n  .van-swipe__indicator {\n    background: gray;\n  }\n}\n</style>\n"]}]}